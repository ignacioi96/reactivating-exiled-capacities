{
  "files.associations": {
    "*.md": "markdown",
    "config.toml": "toml",
    "hugo.toml": "toml"
  },
  "search.exclude": {
    "public/": true,
    "resources/": true,
    ".hugo_build.lock": true,
    "node_modules/": true
  },
  "files.exclude": {
    "public/": true,
    "resources/_gen/": true,
    ".hugo_build.lock": true
  },
  "emmet.includeLanguages": {
    "hugo": "html"
  },
  "hugo.server.renderStaticToDisk": false,
  "git.autofetch": true,
  "markdown.preview.breaks": true
}